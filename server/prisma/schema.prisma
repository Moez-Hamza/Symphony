// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  userName String ?
  level String?
  img   String  @unique
  role  Role    @default(User)
 
}

model singUp {
    id    Int     @id @default(autoincrement())
    email String  @unique
    userName String ?
    password String ?
    img String ?
}


//
enum Role {
  User

}